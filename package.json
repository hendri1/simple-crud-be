{
  "name": "simple-crud-express",
  "version": "1.0.0",
  "description": "Simple CRUD Express",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean && rm -rf dist",
    "start:dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "start:prod": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/server.js",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hendri1/simple-crud-express.git"
  },
  "keywords": [
    "express"
  ],
  "author": "Hendri Faisal Hidayat <hendrifaisal18@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hendri1/simple-crud-express/issues"
  },
  "homepage": "https://github.com/hendri1/simple-crud-express#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^15.0.0",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}
